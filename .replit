modules = ["python-3.11", "nodejs-20", "web"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["geckodriver", "glibcLocales", "xcodebuild", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Mandi Bhav App"

[[workflows.workflow]]
name = "Mandi Bhav App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd nextjs-app && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 32859
externalPort = 3002

[[ports]]
localPort = 36111
externalPort = 5173

[[ports]]
localPort = 37755
externalPort = 4200

[[ports]]
localPort = 42403
externalPort = 5000

[[ports]]
localPort = 43091
externalPort = 6800

[[ports]]
localPort = 45409
externalPort = 3000

[[ports]]
localPort = 46569
