modules = ["python-3.11", "nodejs-20"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["geckodriver", "glibcLocales", "xcodebuild", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "nextjs-app"

[[workflows.workflow]]
name = "nextjs-app"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd nextjs-app && npm run dev -- -p 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34441
externalPort = 3002

[[ports]]
localPort = 35503
externalPort = 6000

[[ports]]
localPort = 35887
externalPort = 3003

[[ports]]
localPort = 36111
externalPort = 5173

[[ports]]
localPort = 37755
externalPort = 4200

[[ports]]
localPort = 42403
externalPort = 5000

[[ports]]
localPort = 43091
externalPort = 6800

[[ports]]
localPort = 45409
externalPort = 3000

[[ports]]
localPort = 46569
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]
